# Database Connections
DATABASE_TYPE=postgres  # postgres, bigquery, snowflake, redshift

# PostgreSQL
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_DB=warehouse
POSTGRES_USER=postgres
POSTGRES_PASSWORD=password

# BigQuery
BIGQUERY_PROJECT_ID=your-project-id
BIGQUERY_CREDENTIALS_PATH=/app/credentials/bigquery.json

# Snowflake
SNOWFLAKE_ACCOUNT=your-account
SNOWFLAKE_USER=your-user
SNOWFLAKE_PASSWORD=your-password
SNOWFLAKE_DATABASE=your-database
SNOWFLAKE_SCHEMA=your-schema
SNOWFLAKE_WAREHOUSE=your-warehouse

# Redshift
REDSHIFT_HOST=your-cluster.redshift.amazonaws.com
REDSHIFT_PORT=5439
REDSHIFT_DB=dev
REDSHIFT_USER=your-user
REDSHIFT_PASSWORD=your-password

# AI Configuration
GOOGLE_API_KEY=your-gemini-api-key
GEMINI_MODEL=gemini-1.5-pro

# Vector Store (pgvector)
VECTOR_DB_HOST=postgres
VECTOR_DB_PORT=5432
VECTOR_DB_NAME=vector_store
VECTOR_DB_USER=postgres
VECTOR_DB_PASSWORD=password

# Security
SECRET_KEY=your-secret-key-here-change-in-production
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:8000

# Logging
LOG_LEVEL=INFO
ENABLE_QUERY_LOGGING=true

# Query Limits
MAX_QUERY_ROWS=10000
QUERY_TIMEOUT_SECONDS=300